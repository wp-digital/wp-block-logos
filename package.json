{
	"name": "wp-block-logos",
	"version": "1.1.0",
	"description": "List of logos with links.",
	"author": "Innocode",
	"license": "GPL-2.0-or-later",
	"main": "build/index.js",
	"scripts": {
		"build:logo": "wp-scripts build blocks/logo/src/index.js --output-path=blocks/logo/build/",
		"build:logos": "wp-scripts build blocks/logos/src/index.js blocks/logos/src/view.js --output-path=blocks/logos/build/",
		"build": "npm run build:logo && npm run build:logos",
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style",
		"lint:css:fix": "wp-scripts lint-style --fix",
		"lint:js": "wp-scripts lint-js",
		"lint:js:fix": "wp-scripts lint-js --fix",
		"start:logo": "wp-scripts start blocks/logo/src/index.js --output-path=blocks/logo/build/",
		"start:logos": "wp-scripts start blocks/logos/src/index.js --output-path=blocks/logos/build/",
		"packages-update": "wp-scripts packages-update"
	},
	"dependencies": {
		"@innocode-digital/wp-component-icons-picker": "^1.1.1",
		"@wordpress/block-editor": "^9.5.0",
		"@wordpress/blocks": "^11.12.0",
		"@wordpress/components": "^19.15.0",
		"@wordpress/dom-ready": "^3.13.0",
		"@wordpress/element": "^4.11.0",
		"@wordpress/hooks": "^3.13.0",
		"@wordpress/i18n": "^4.13.0",
		"@wordpress/icons": "^9.4.0",
		"@wordpress/primitives": "^3.11.0",
		"lodash": "^4.17.21"
	},
	"devDependencies": {
		"@wordpress/scripts": "^23.5.0",
		"prettier": "^2.5.1"
	}
}
